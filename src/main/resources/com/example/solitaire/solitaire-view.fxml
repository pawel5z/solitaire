                  </toggleGroup>
               </RadioMenuItem>
               <RadioMenuItem fx:id="europeanBoardRadio" mnemonicParsing="false" onAction="#boardTypeToggled" text="European board" toggleGroup="$boardTypeToggleGroup" />
